{
  "private": true,
  "scripts": {
    "build": "npm run build:ts && npm run build:fiols && nitro build",
    "build:ts": "rollup -c --configPlugin typescript --no-watch",
    "build:fiols": "jiti cli/cli.ts gen",
    "watch:ts": "rollup -c --configPlugin typescript --watch",
    "watch:fiols": "jiti cli/cli.ts gen --watch",
    "dev": "jiti scripts/dev.ts",
    "dev:server": "nitro dev",
    "dev:fake3p": "cross-env PORT=3001 nitro dev --dir fake3p",
    "prepare": "npm run build:ts && npm run build:fiols && nitro prepare",
    "preview": "node .output/server/index.mjs"
  },
  "devDependencies": {
    "nitropack": "latest"
  },
  "dependencies": {
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-typescript": "^12.1.1",
    "citty": "^0.1.6",
    "cross-env": "^7.0.3",
    "jiti": "^2.3.3",
    "pyodide": "^0.26.3",
    "rollup": "^4.24.2",
    "tslib": "^2.8.0",
    "typescript": "^5.6.3"
  },
  "packageManager": "pnpm@9.12.2+sha512.22721b3a11f81661ae1ec68ce1a7b879425a1ca5b991c975b074ac220b187ce56c708fe5db69f4c962c989452eee76c82877f4ee80f474cebd61ee13461b6228"
}
